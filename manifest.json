{
  "manifest_version": 2,
  "name": "Trasmontano Fixer",
  "version": "1.0.0",

  "description": "Corrige problemas de usabilidade no site da Trasmontano",

  "browser_specific_settings": {
    "gecko": {
      "id": "trasmontano-fixer@davi.dev"
    }
  },

  "content_scripts": [
    {
      "matches": ["*://*.trasmontano.com.br/*"],
      "js": ["dist/trasmontano.js"]
    }
  ]

}
